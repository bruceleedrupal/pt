<form method="post" action="{{ path('agent_package_size_delete', {'id': package_size.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ package_size.id) }}">
    <button class="btn">Delete</button>
</form>
